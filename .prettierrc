{
  "arrowParens": "always",
  "bracketSpacing": true,
  "insertPragma": false,
  "bracketSameLine": true,
  "printWidth": 100,
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrder": ["^@/(.*)$", "^../", "^./"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "endOfLine": "auto"
}
